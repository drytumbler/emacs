 
#+begin_src elisp 

(defun display-color (name color)
    "Insert a string with the specified COLOR."
    (interactive "sEnter color name or hex code: ")
    (let ((text (format "This is the color %s: %s" name color))
          (face `((t (:foreground ,color)))))
      (insert (propertize text 'face face))
      (insert "\n")))

  (defun display-color-block (color)
    "Insert a block of text with the specified background COLOR."
    (interactive "sEnter color name or hex code: ")
    (let ((text "      ")
          (face `((t (:background ,color)))))
      (insert (propertize text 'face face))
      (insert "\n")))

#+end_src

