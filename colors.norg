#+begin_src  elisp

(defun process-color (color-pair)
    "Process each color in the list."
    (let ((color (car color-pair))
          (name (cdr color-pair)))
      ;; Replace this with the function you want to run on each color
      (display-color name color)
      (display-color-block color)))

#+end_src

